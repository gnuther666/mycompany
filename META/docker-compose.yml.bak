version: "1.0"
services:
  backend:
    build: ./backend
    ports:
      - 8010:22
      - 8000:8000
    volumes:
      - E:\workspace\mycompany\backend:/app
    environment:
      -TZ=Asia/Shanghai
  frontend:
    build: ./frontend_dev
    ports:
      - 8011:22
      - 8002:8000
    volumes:
      - E:\workspace\mycompany\frontend:/app
    environment:
      -TZ=Asia/Shanghai
